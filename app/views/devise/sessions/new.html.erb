<h2>ಪ್ರವೇಶ</h2>

<%= form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f| %>
<div> ಇಮೇಲ್ <br />
  <%= f.email_field :email, :autofocus => true %></div>

  <div> ಗುಪ್ತಪದ <br />
    <%= f.password_field :password %></div>

    <div><%= f.submit "ಪ್ರವೇಶ" %></div>
    <% end %>



    <%= render "devise/shared/links" %>
