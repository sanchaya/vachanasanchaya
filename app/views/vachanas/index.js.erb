$('.vachanas_load').append("<%= escape_javascript(render(partial: 'results')) %>");
<% params.delete(:page) if params[:page] %>
$('a.load-more-vachanas').attr('href',"<%= vachanas_path(params: params, :page => @results.next_page) %>" );
<% unless @results.next_page %>
  $('a.load-more-vachanas').remove();
<% end %>